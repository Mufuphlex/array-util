{
  "name": "mufuphlex/array-util",
  "type": "library",
  "version": "1.0.0",
  "description": "Array utils - smart&fast array helpers",
  "keywords": [
    "array",
    "array util",
    "array walk",
    "array map",
    "array cut",
    "array intersect",
    "array unique",
    "smart intersect",
    "smart unique",
    "fast intersect",
    "fast unique"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "AlexMiroshnikov"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "Mufuphlex\\Util\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=9",
    "friendsofphp/php-cs-fixer": "^3.15"
  },
  "scripts": {
    "test-with-coverage-text": "vendor/bin/phpunit --configuration phpunit.xml.dist --coverage-text"
  }
}
